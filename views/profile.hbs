{{#if photo}}
<img src="{{photo}}" class="mr-3" alt="">
{{/if}}
<p>{{user.username}}</p>
<p>Почта: {{user.email}}</p>
{{#with profile}}
<p>Фамилия и имя: {{first_name}} {{last_name}}</p>
<p>Дата рождения: {{formatDate birthday day="numeric" month="numeric" year="numeric"}}</p>
<p>Дата регистрации: {{formatDate dateOfRegistration day="numeric" month="numeric" year="numeric"}}</p>
{{/with}}